<p id="notice"><%= notice %></p>

<h1>Results</h1>

<table>
  <thead>
    <tr>
      <th>Home</th>
      <th>Hg</th>
      <th>Ag</th>
      <th>Away</th>
      <th>Time</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @results.each do |result| %>
      <tr>
        <td><%= result.home %></td>
        <td><%= result.hg %></td>
        <td><%= result.ag %></td>
        <td><%= result.away %></td>
        <td><%= result.time %></td>
        <td><%= link_to 'Edit', fixtures_edit_path(result) %></td>

      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Result', fixtures_new_path %>
<p> </p>
<%= link_to 'Back', :back %>